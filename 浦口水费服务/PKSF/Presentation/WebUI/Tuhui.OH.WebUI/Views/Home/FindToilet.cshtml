<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title></title>
   <script src="@Url.Content("~/Script/jquery-1.8.2.js")" type="text/javascript"></script>
    <script type="text/javascript">
            
    </script>
    <style type="text/css">
        body{margin: 0px;padding: 0px;background: #ffffff;}
        #box_title{ height: 50px; line-height: 50px; font-size: 22px; background: url('@Url.Content("~/content/title/tit_back.png")') repeat-x;color: #fff;font-family: '微软雅黑', '黑体';}
        #box_title .titleIcon{width: 126px;height: 31px;background: url('@Url.Content("~/content/title/tit_12.png")') no-repeat center;position: absolute;left: 0px;top: 0px;margin-left: 50px;margin-top: 12px;}
      @*  #box_title .titleContent{width: 280px;height: 50px;position: absolute;left: 90px;top: 0px;}*@
        #box_title .closeBtn{width: 21px;height: 21px;background: url('@Url.Content("~/content/title/close.png")') no-repeat center;position: absolute;right: 0px;top: 0px; cursor: pointer;margin-top: 12px;margin-right: 15px;}
        #box_list ul li{ width: 220px;height: 105px;line-height: 105px;color: #fff;font-size: 28px;font-family: '微软雅黑', '黑体'; font-weight: bold;float: left;background: #58a133;text-align: center;margin: 30px 20px 20px 10px;cursor: pointer;}
     
         #box_list ul li:hover {
             background: #d06003;
         }
    </style>

    <script type="text/javascript">
        $(function () {

            $("#box_list ul li").on("click", function (e) {
                //if (console) {
                //    console.log("当前点击data：" + e.target.getAttribute("data"));
                //}


                //TODO:这里写调用方法
                window.parent.$.OH.findToilet.findWashRoomById(e.target.getAttribute("data"),1);
                window.parent.$("#FindToilet").hide();
                window.parent.globalToiletId = e.target.getAttribute("data");//全局变量
                window.parent.globalType = 1;
            });

            //关闭按钮
            $(".closeBtn").on("click", function () {
                window.parent.$("#FindToilet").hide();
            });
        });

    </script>
</head>
<body>
    <div id="box_title">
        <div class="titleIcon"></div>
        <div style="background: #cbdcc3; width: 60px; height: 50px; float: right">
            <div class="closeBtn"></div>
        </div>

    </div>
    <div id="box_list">
        <ul>
             <li id="toilet1" data="1">卫生间1</li>
            <li id="toilet2" data="2">卫生间2</li>
            <li id="toilet3" data="3">卫生间3</li>
            <li id="toilet4" data="4">卫生间4</li>
            <li id="toilet5" data="5">卫生间5</li>
            <li id="toilet6" data="6">卫生间6</li>
            <li id="toilet7" data="7">卫生间7</li>
            <li id="toilet8" data="8">卫生间8</li>
        </ul>
    </div>
</body>
</html>

